<UserControl x:Class="InteractServer.CodeEditor.FallbackEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InteractServer.CodeEditor"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<DockPanel LastChildFill="True">
		<TextBox x:Name="codeEditor" FontFamily="Consolas" FontSize="14" TextChanged="codeEditor_TextChanged">

		</TextBox>
	</DockPanel>
</UserControl>
